    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "mocha": "^8.1.1",
    "prettier": "^2.1.0",
    "should": "^13.2.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "the-lodash": "^2.0.9",
    "the-logger": "^2.0.14",
    "the-promise": "^2.0.15",
