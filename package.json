{
  "name": "kubevious-cli",
  "version": "1.0.0",
  "description": "Kubevious CLI",
  "main": "dist/index.js",
  "types": "dist",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "build": "tsc",
    "format": "prettier --write ./src/ ./test/",
    "format-check": "prettier --write ./src/ ./test/",
    "lint": "eslint",
    "start": "tsc && node dist/index.js"
  },
  "author": "Ruben Hakopian <ruben.hakopian@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kubevious/kubevious/issues"
  },
  "homepage": "https://github.com/kubevious/kubevious",
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "cli-table": "^0.3.11",
    "commander": "^9.4.1",
    "express-openapi-validate": "^0.6.1",
    "glob": "^8.0.3",
    "js-yaml": "^4.1.0",
    "k8s-super-client": "^2.0.34",
    "lodash": "^4.17.21",
    "openapi-schema-validator": "^12.0.2",
    "the-lodash": "^2.0.9",
    "the-logger": "^2.0.14",
    "the-promise": "^2.0.15",
